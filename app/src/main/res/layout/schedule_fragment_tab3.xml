<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:paddingBottom="50px"
        android:paddingLeft="100px"
        android:paddingRight="150px"
        android:paddingTop="100px"


        >


        <TableRow>

            <TextView
                android:layout_column="0"
                android:padding="11dip"
                android:text="09:30"
                android:textSize="18sp" />

            <TextView
                android:padding="11dip"
                android:text="Discussion Groups"
                android:textSize="18sp" />
        </TableRow>
        <!-- Linha separadora -->
        <View
            android:layout_height="2dip"
            android:background="#FF909090" />

        <TableRow>

            <TextView
                android:layout_column="0"
                android:padding="11dip"
                android:text="10:45"
                android:textSize="18sp" />

            <TextView
                android:padding="11dip"
                android:text="Coffee-Break"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_height="2dip"
            android:background="#FF909090" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_column="0"
                android:padding="11dip"
                android:text="11:00"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView"
                android:padding="11dip"
                android:text="Discussion Groups"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_height="2dip"
            android:background="#FF909090" />

        <TableRow>

            <TextView
                android:padding="11dip"
                android:text="12:30"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView6"
                android:padding="11dip"
                android:text="Lunch"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_height="2dip"
            android:background="#FF909090" />

        <TableRow>

            <TextView
                android:id="@+id/textView4"
                android:layout_column="0"
                android:padding="11dip"
                android:text="14:00"
                android:textSize="18sp" />

            <TextView
                android:layout_column="0"
                android:padding="11dip"
                android:text="Speeches"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_height="2dip"
            android:background="#FF909090" />

        <TableRow>

            <TextView
                android:layout_column="0"
                android:padding="11dip"
                android:text="16:00"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_column="1"
                android:padding="11dip"
                android:text="Coffee Break"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_height="2dip"
            android:background="#FF909090" />

        <TableRow>

            <TextView
                android:layout_column="0"
                android:padding="11dip"
                android:text="16:30"
                android:textSize="18sp" />

            <TextView
                android:layout_column="1"
                android:padding="11dip"
                android:text="Round-Table"
                android:textSize="18sp" />
        </TableRow>

        <View
            android:layout_height="2dip"
            android:background="#FF909090" />

        <TableRow>

            <TextView
                android:layout_column="0"
                android:padding="11dip"
                android:text="18:00"
                android:textSize="18sp" />
            <TextView
                android:layout_column="1"
                android:padding="11dip"
                android:text="Closing Session"
                android:textSize="18sp" />


        </TableRow>



    </TableLayout>

</LinearLayout>